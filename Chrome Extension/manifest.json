{
    "manifest_version": 3, 
    "name": "Website Tracker",
    "version": "0.0",
    "description": "Tracks Websites You've Visited",

    "action": {
        "default_title": "Website Tracker",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },

    "permissions": ["activeTab", "tabs", "storage"]
}